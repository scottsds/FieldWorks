	</part>
	<part ref="DescriptionSummary" param="DescriptionRDE" label="Description">
	</part>
	<part ref="QuestionsRDE">
	</part>
  </layout>

 <layout class="CmDomainQ" type="detail" name="default">
	<part ref="QuestionAllA" expansion="expanded">
		<indent>
			<part ref="ExampleWordsAllA"/>
			<part ref="ExampleSentencesAllA"/>
		</indent>
	</part>
  </layout>

 <layout class="CmDomainQ" type="detail" name="RDE">
	<part ref="QuestionSummary" param="QuestionRDE" label="Question">
		<indent>
			<part ref="ExampleWordsSummary" param="ExampleWordsRDE" label="Example Words"/>
			<part ref="ExampleSentencesSummary" param="ExampleSentencesRDE" label="Example Sentences"/>
		</indent>
	</part>
  </layout>


  <layout class="LexEntryType" type="detail" name="EditComplex">
	<part ref="NameAllA"/>
