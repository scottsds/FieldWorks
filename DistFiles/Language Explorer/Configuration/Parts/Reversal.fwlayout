	<layout class="ReversalIndexEntry" type="detail" name="Normal">
		<part ref="ChangeHandler"/>
		<part ref="ReversalForm" label="Reversal Form" menu="mnuDataTree-MoveReversalIndexEntry"/>
		<part ref="PartOfSpeech"/>
		<part ref="CurrentSenses"/>
		<part ref="Section" expansion="expanded" label="Subentries" menu="mnuDataTree-InsertReversalSubentry" hotlinks="mnuDataTree-InsertReversalSubentry-Hotlinks">
			<indent indent="true">
				<part ref="ReversalSubentries" param="Subentry"/>
			</indent>
		</part>
	</layout>
	<layout class="ReversalIndexEntry" type="detail" name="Subentry">
		<part ref="ReversalForm" label="Reversal Form" menu="mnuDataTree-MoveReversalIndexEntry"/>
		<part ref="PartOfSpeech"/>
		<part ref="CurrentSenses"/>
		<part ref="Section" expansion="expanded" label="Subentries" menu="mnuDataTree-InsertReversalSubentry" hotlinks="mnuDataTree-InsertReversalSubentry-Hotlinks">
			<indent indent="true">
				<part ref="ReversalSubentries" param="Subentry"/>
			</indent>
		</part>
	</layout>
	<layout class="PartOfSpeech" type="detail" name="Reversal">
		<part ref="NameRevPOS" menu="mnuDataTree-MoveMainReversalPOS"/>
		<part ref="AbbreviationRevPOS"/>
		<part ref="DescriptionAllA"/>
		<part ref="Section" expansion="expanded" label="Subcategories" menu="mnuDataTree-POS-SubPossibilities">
			<indent indent="true">
				<part ref="SubPossibilitiesReversal" label="Subcategories"/>
			</indent>
		</part>
	</layout>
	<layout class="PartOfSpeech" type="detail" name="ReversalSubcategory">
		<part ref="NameRevPOS" menu="mnuDataTree-MoveReversalPOS"/>
		<part ref="AbbreviationRevPOS"/>
		<part ref="DescriptionAllA"/>
		<part ref="Section" expansion="expanded" label="Subcategories" menu="mnuDataTree-POS-SubPossibilities">
			<indent indent="true">
				<part ref="SubPossibilitiesReversal" label="Subcategories"/>
			</indent>
		</part>
	</layout>

	<layout class="ReversalIndexEntry" type="jtview" name="publishReversal" tagForWs="true">
		<part ref="ReversalConfig" param="publishReversalIndexEntry"/>
	</layout>

	<layout class="ReversalIndexEntry" type="jtview" name="publishReversalIndexEntry" tagForWs="true">
		<sublayout name="publishPara" group="para" style="Dictionary-Normal"/>
		<part ref="SubentriesConfig" param="publishReversalIndexEntry" label="Reversal Subentries" parastyle="Dictionary-Subentry" flowType="div" visibility="ifdata" recurseConfig="false" css="subentries"/>
	</layout>
	<layout class="ReversalIndexEntry" type="jtview" name="publishPara" tagForWs="true" css="$fwstyle=Dictionary-Normal">
